<%- include("partials/header") %>

<h1>Home</h1>
<p> <%= homeStartingContent %> </p>

<% posts.forEach(function(post) { %>
    <h2><%= post.title %></h2>
    <!-- <p><%= post.text %></p> -->
<% if(post.text.length > 100) { %>
        <p><%= post.text.substring(0, 100) + "..." %></p>
<%   } else { %>
        <p><%= post.text %></p>
<%    } %>
<a href="/posts/<%= post.title %>">Read More</a>
<% }); %>

<%- include("partials/footer") %>